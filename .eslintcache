);\n\n  useEffect(() => {\n    const { role } = JSON.parse(localStorage.getItem(\"user\"));\n    if (role === 1) {\n      setIsUser(true);\n    }\n  }, []);\n\n  const logoutHandler = () => {\n    localStorage.removeItem(\"user\");\n    history.push(\"/\");\n    notification(\"success\", \"Успешный выход\", `Вы вышли из аккаунта!`);\n  };\n\n  return (\n    <Sider trigger={null} collapsible collapsed={collapsed}>\n      <div\n        className=\"logo\"\n        style={{ cursor: \"pointer\" }}\n        onClick={() => history.push(\"/home\")}\n      >\n        <CardLogo />\n      </div>\n      <Menu theme=\"dark\" mode=\"inline\" defaultSelectedKeys={[\"1\"]}>\n        <Menu.Item key=\"1\" icon={<BookOutlined />}>\n          <Link to=\"/home\">Главная</Link>\n        </Menu.Item>\n        {!isUser && (\n          <Menu.Item key=\"3\" icon={<UserOutlined />}>\n            <Link to=\"/add-new-patient\">Добавить пациента</Link>\n          </Menu.Item>\n        )}\n        <Menu.Item\n          key=\"4\"\n          icon={<LogoutIcon />}\n          style={{ display: \"flex\", alignItems: \"center\" }}\n        >\n          <button className=\"exit-button\" onClick={logoutHandler}>\n            Выйти\n          </button>\n        </Menu.Item>\n      </Menu>\n    </Sider>\n  );\n};\n\nexport default BooksNav;\n","/Users/tetrafour/Documents/cours/src/Components/DiseaseList/index.js",[],"/Users/tetrafour/Documents/cours/src/Components/AuthProvider/index.js",["119"],"/Users/tetrafour/Documents/cours/src/routes/private/index.js",[],"/Users/tetrafour/Documents/cours/src/routes/public/index.js",[],"/Users/tetrafour/Documents/cours/src/Pages/Test/index.js",[],"/Users/tetrafour/Documents/cours/src/hooks/index.js",[],"/Users/tetrafour/Documents/cours/src/Pages/Auth/index.js",[],"/Users/tetrafour/Documents/cours/src/Components/Header/index.js",[],"/Users/tetrafour/Documents/cours/src/routes/Private/index.js",[],"/Users/tetrafour/Documents/cours/src/routes/Public/index.js",[],{"ruleId"